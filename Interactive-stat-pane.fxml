<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<Pane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InteractiveStatController">
   <children>
      <BorderPane>
         <top>
            <HBox BorderPane.alignment="CENTER">
               <children>
                  <BorderPane>
                     <left>
                        <ComboBox fx:id="boroughs" onAction="#processBoroughsBox" prefWidth="150.0" BorderPane.alignment="CENTER" />
                     </left>
                     <center>
                        <ComboBox fx:id="propertyName" disable="true" onAction="#processPropertiesBox" prefWidth="150.0" BorderPane.alignment="CENTER" />
                     </center>
                     <right>
                        <ComboBox fx:id="price" disable="true" onAction="#processPriceBox" prefWidth="150.0" BorderPane.alignment="CENTER" />
                     </right>
                  </BorderPane>
               </children>
            </HBox>
         </top>
         <center>
            <VBox BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</Pane>
